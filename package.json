{
  "name": "@ucdjs/release-scripts",
  "version": "0.1.0-beta.3",
  "description": "@ucdjs release scripts",
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucdjs/release-scripts.git"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commit-parser": "0.4.5",
    "debug": "4.4.3",
    "farver": "1.0.0-beta.1",
    "prompts": "2.4.2",
    "tinyexec": "1.0.2"
  },
  "devDependencies": {
    "@luxass/eslint-config": "6.0.1",
    "@types/debug": "4.1.12",
    "@types/node": "22.18.12",
    "@types/prompts": "2.4.9",
    "eslint": "9.39.1",
    "tsdown": "0.16.0",
    "typescript": "5.9.3",
    "vitest": "4.0.4"
  }
}
