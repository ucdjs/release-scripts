{
  "name": "@ucdjs/release-scripts",
  "version": "0.1.0-beta.24",
  "description": "@ucdjs release scripts",
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucdjs/release-scripts.git"
  },
  "imports": {
    "#services/*": "./src/services/*.service.ts"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@effect/platform": "0.93.6",
    "@effect/platform-node": "0.103.0",
    "@luxass/utils": "2.7.2",
    "commit-parser": "1.3.0",
    "effect": "3.19.9",
    "farver": "1.0.0-beta.1",
    "mri": "1.2.0",
    "prompts": "2.4.2",
    "semver": "7.7.3",
    "tinyexec": "1.0.2"
  },
  "devDependencies": {
    "@effect/language-service": "^0.60.0",
    "@effect/vitest": "0.27.0",
    "@luxass/eslint-config": "6.0.3",
    "@types/node": "22.18.12",
    "@types/prompts": "2.4.9",
    "@types/semver": "7.7.1",
    "eslint": "9.39.1",
    "eta": "4.4.1",
    "tsdown": "0.17.0",
    "typescript": "5.9.3",
    "vitest": "4.0.15",
    "vitest-testdirs": "4.3.0"
  }
}
